{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "dist/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "./src/(.*)",
            "dest": "dist/index.js"
        }
    ]
}
